{% extends 'base.html.twig' %}

{% block title %}Details Administrateur{% endblock %}

{% block body %}
    <h1>Administrateur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ administrateur.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ administrateur.email }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ administrateur.password }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('administrateur_index')}}">Retour</a>

    <a href="{{ path('administrateur_edit', {'id': administrateur.id})}}">Modifier</a>

    {{ include('administrateur/_delete_form.html.twig')}}
{% endblock %}
